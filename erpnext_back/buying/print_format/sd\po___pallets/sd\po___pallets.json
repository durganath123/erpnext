 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead, footer) -%}\n    <div class=\"leader-head\">\n        <table style=\"border-bottom:1px solid lightgray; margin:0px !important; padding:0px !important; width:100%\">\n            <tbody>\n                <tr style=\"padding:0px !important; margin:0px !important\">\n                    <td style=\"padding:0px !important; margin:0px !important\" width=\"50%\" class=\"text-left\">\n                        {% if not no_letterhead %}\n                            <img style=\"width:200px !important\" src=\"/files/Sundine_LOGO_FINAL_large.png\" width=\"42%\" /> <br/>\n                            <p style=\"font-size:11px;\">6075 Kestrel Rd, Mississauga, ON L5T 1Y8 Canada <br/>\n                            Ph: 905 364 1600 | Fax: 416 342 1763    <br/>   \n                            Email: sales@sundineproduce.com  |  www.sundineproduce.com</p>\n                        {% endif %}\n                    </td>\n                    <td style=\"padding:0px !important; margin:0px !important\" width=\"50%\" align=\"right\">\n                        <div class=\"text-right\"><h3>PURCHASE ORDER</h3></div>\n                        <div class=\"text-right\"> <big>{{ doc.name }}</big></div>\n                        <div class=\"text-right\">Shipping Date: <b>{{ frappe.format_date(doc.shipping_date, \"EEE, d-MMM-Y\") }}</b></div>\n                        <div class=\"text-right\">Shipping Mode: <b>{{ doc.order_type or \"\" }}</b></div>\n\t\t\t{% if doc.note %}<div class=\"text-right\">Note: <b>{{ doc.note }}</b></div>{% endif %}\n                        <!--{% if max_pages > 1 %}\n                            <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n                        {% endif %}\n                        <p class=\"text-right small page-number visible-pdf\">\n                            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n                        </p>-->\n                    </td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n{%- endmacro -%}\n\n{{ add_header(1, layout|len, doc, letter_head, no_letterhead, footer) }}\n\n<small>\n\n<div class=\"row\">        \n    <div class=\"col-xs-6 text-left\"> \n        <p style=\"font-size:14px; margin-top: 14px;\">Supplier:</p> \n        <h3>{{ doc.supplier_name or '' }}</h3>\n    </div>\n    <div class=\"col-xs-6\">\n        <p style=\"font-size:14px; margin-top: 14px;\">{{ doc.address_display or \"\" }}</p>\n    </div>\n</div>\n\n<div class=\"clearfix\"></div>\n\n<table class=\"table table-condensed table-hover table-bordered\">\n    <tbody>\n        <tr>\n            <th class=\"text-center\" stye=\"width:7%\">Pallets</th>\n            <th class=\"text-center\" stye=\"width:15%\" colspan=\"2\">Qty</th>\n            <th class=\"text-center\" stye=\"width:42%\">Product</th>\n            <th class=\"text-center\" stye=\"width:7%\">Qty/Pallet</th>\n            <th class=\"text-center\" stye=\"width:15%\">Unit Qty</th>\n            <th class=\"text-center\" stye=\"width:7%\">GW (lbs)</th>\n            <th class=\"text-center\" stye=\"width:7%\">GW (Kg)</th>\n        </tr>\n        {%- for row in doc.items -%}\n            {% if row.qty %}\n            <tr>\n                <td style=\"text-align: center;\" class=\"text-nowrap\">{{ row.get_formatted('pallets') }}</td>\n                <td style=\"text-align: center; width:7%;\" class=\"text-nowrap\"><b>{{ row.get_formatted('qty') }}</b></td>\n                <td style=\"text-align: center; width:8%;\" class=\"text-nowrap\"><b>{{ row.uom }}</b></td>\n                <td style=\"text-align: left;\">{{ row.item_name or ''}}</td>\n                <td style=\"text-align: center;\" class=\"text-nowrap\">{{ row.get_formatted('qty_per_pallet') }}</td>\n                <td style=\"text-align: center;\" class=\"text-nowrap\">{{ row.get_formatted('alt_uom_size') }} {{ row.alt_uom or '' }}</td>\n                <td style=\"text-align: center;\" class=\"text-nowrap\">{{ row.get_formatted('total_weight') }}</td>\n                <td style=\"text-align: center;\" class=\"text-nowrap\">{{ row.get_formatted('total_weight_kg') }}</td>\n            </tr>\n            {% endif %}\n        {%- endfor -%}\n\n        <tr>\n            <th class=\"text-center\">{{ doc.get_formatted('total_pallets') }}</th>\n            <th class=\"text-center\">{{ doc.get_formatted('total_qty') }}</th>\n            <th class=\"text-center\"></th>\n            <th class=\"text-center\"></th>\n            <th class=\"text-left\"></th>\n            <th class=\"text-center\"></th>\n            <th class=\"text-center\">{{ doc.get_formatted('total_gross_weight') }}</th>\n            <th class=\"text-center\">{{ doc.get_formatted('total_gross_weight_kg') }} </th>\n        </tr>\n    </tbody>\n</table>\n</small>", 
