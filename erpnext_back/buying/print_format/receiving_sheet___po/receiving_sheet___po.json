 "html": "<div class=\"page-break1\">\n\t<small>\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-6 text-left\">\n\t\t\t\t<h3>{{ doc.supplier_name or doc.supplier }}</h3>\n\t\t\t\t<strong>Actual No of Pallets:</strong> {{ doc.get_formatted('total_packed_pallets') }}\n\t\t\t</div>\n\t\t\t<div class=\"col-xs-6 text-right\">\n\t\t\t\t<h3>{{ doc.select_print_heading or \"Purchase Receiving Sheet\" }}</h3>\n\t\t\t\t<big>{{ doc.name or '' }}</big><br />\n\t\t\t\t<big>Date: <b>{{ frappe.format_date(doc.schedule_date, \"d-MMM-Y\") }}</b></big>\n\t\t\t\t{% if doc.note %}<br /><big>Note: <b>{{ doc.note }}</b></big>{% endif %}\n\t\t\t</div>\n\t\t</div>\n\t\t<br>\n\n\t\t<table class=\"table table-condensed table-hover table-bordered\">\n\t\t\t<tbody>\n\t\t\t\t<tr>\n\t\t\t\t\t<th class=\"text-left\" style=\"width: 12%;\">Item Code</th>\n\t\t\t\t\t<th class=\"text-left\" style=\"width: 38%;\">Name</th>\n\t\t\t\t\t<th class=\"text-center\" style=\"width: 10%;\">Ordered</th>\n\t\t\t\t\t<th class=\"text-center\" style=\"width: 10%;\">Received</th>\n\t\t\t\t\t<th colspan=\"3\" class=\"text-center\" style=\"width: 30%;\">Credits</th>\n\t\t\t\t</tr>\n\t\t\t\t{% set total_qty = namespace(b=0) %}\n\t\t\t\t{%- for row in doc.items -%}\n\t\t\t\t\t{% if row.qty %}\n\t\t\t\t\t{% set total_qty.b = total_qty.b+row.qty|int %}\n\t\t\t\t\t<tr>\n\t\t\t\t\t\t<td>{{ row.item_code or ''}}</td>\n\t\t\t\t\t\t<td>{{ row.item_name or '' }}</td>\n\t\t\t\t\t\t<td style=\"text-align: center;\">{{ row.get_formatted('qty') }}</td>\n\t\t\t\t\t\t<td style=\"text-align: center;\"></td>\n\t\t\t\t\t\t<td style=\"text-align: center;\"></td>\n\t\t\t\t\t\t<td style=\"text-align: center;\"></td>\n\t\t\t\t\t\t<td style=\"text-align: center;\"></td>\n\t\t\t\t\t</tr>\n\t\t\t\t\t{% endif %}\n\t\t\t\t{%- endfor -%}\n\t\t\t\t<tr>\n\t\t\t\t\t<th colspan=\"2\" class=\"text-right\">Total:</th>\n\t\t\t\t\t<th class=\"text-center\">{{ doc.get_formatted('total_qty') }}</th>\n\t\t\t\t\t<th class=\"text-right\"></th>\n\t\t\t\t\t<th class=\"text-right\"></th>\n\t\t\t\t\t<th class=\"text-right\"></th>\n\t\t\t\t\t<th class=\"text-right\"></th>\n\t\t\t\t</tr>\n\t\t\t</tbody>\n\t\t\t\n\t\t</table>\n\t\t<tr>\n\t\t<td><big>Received By:</big></td>\n\t\t\t</tr>\n\t</small>\n\n\n</div>",
